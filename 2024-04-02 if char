#define _CRT_SECURE_NO_WARNINGS

#include <stdio.h>

int main(void)  {
    int x=0, y=0;
    char op;
    
    scanf("%d %c %d", &x, &op, &y);
    
    if (op == '+')    {
        printf("%d %c %d = %d", x, op, y, x+y);
    }
    else if (op == '-')    {
        printf("%d %c %d = %d", x, op, y, x-y);
    }
    else if (op == '*')    {
        printf("%d %c %d = %d", x, op, y, x*y);
    }
    else if (op == '/')    {
        printf("%d %c %d = %d", x, op, y, x/y);
    }
    else    {
        printf("잘못된 연산자를 입력했습니다.");
    }
    
    return 0;
}
